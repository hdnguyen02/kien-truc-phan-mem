# .gitignore

# Ignore node_modules folder in the frontend directory
/frontend/node_modules

# Ignore build outputs in the frontend directory
/frontend/build
/frontend/dist

# Ignore npm and yarn lock files in the frontend directory
/frontend/package-lock.json
/frontend/yarn.lock

# Ignore dependency directories in the backend directory
/backend/target
/backend/out

# Ignore IDE specific files
*.idea/
*.vscode/
*.iml

# Ignore OS specific files
.DS_Store
Thumbs.db

# Ignore log files and databases
*.log
*.sql
*.sqlite

# Ignore environment variables files
.env
.env.local

# Java specific ignores in backend directory
/backend/.classpath
/backend/.project
/backend/.settings/
*.class
*.jar
*.war
*.ear
*.nar
*.har
*.zip
*.tar.gz
*.rar
*.exe
*.dll
*.com
*.so
*.o
*.a
*.pyc
*.pyo
*.pyd
*.pid
*.log

# Compiled class file
/backend/**/*.class

# Compiled JAR file
/backend/**/*.jar

# Compiled WAR file
/backend/**/*.war

# Compiled EAR file
/backend/**/*.ear

# Compiled binary log
/backend/**/*.bin

# BlueJ files
/backend/*.ctxt

# Mobile Tools for Java (J2ME)
.backend/.mtj.tmp/

# Package Files #
*.jar
*.war
*.nar
*.ear
*.zip
*.tar.gz
*.rar

# virtual machine crash logs, see http://www.java.com/en/download/help/error_hotspot.xml
hs_err_pid*
